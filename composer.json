{
    "authors": [
        {
            "email": "minottoemanuele@gmail.com",
            "name": "Emanuele Minotto"
        }
    ],
    "autoload": {
        "psr-4": {
            "EmanueleMinotto\\DomainParserBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EmanueleMinotto\\DomainParserBundle\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "description": "Symfony Bundle for jeremykendall/php-domain-parser",
    "extra": {
        "require": "^4.4",
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "keywords": [
        "domain",
        "parser",
        "bundle"
    ],
    "license": "MIT",
    "name": "emanueleminotto/domain-parser-bundle",
    "require": {
        "php": ">= 7.4",
        "jeremykendall/php-domain-parser": "^5.7",
        "matthiasnoback/symfony-dependency-injection-test": "^4.3",
        "symfony/console": ">=5.4",
        "symfony/framework-bundle": ">= 5.4"
    },
    "require-dev": {
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^9.0",
        "rector/rector": "^0.13.8",
        "scrutinizer/ocular": "^1.3",
        "squizlabs/php_codesniffer": "^2.5",
        "symfony/translation": ">= 5.4",
        "symfony/twig-bridge": ">= 5.4",
        "symfony/validator": ">= 5.4",
        "symfony/yaml": ">= 5.4",
        "twig/twig": "^3.4"
    },
    "scripts": {
        "test": "phpunit"
    },
    "type": "symfony-bundle"
}
